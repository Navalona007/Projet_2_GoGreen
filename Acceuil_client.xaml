<Window x:Class="Projet_2_GoGreen.Acceuil_client"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projet_2_GoGreen"
        mc:Ignorable="d"
        Title="Acceuil_client" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="21" Margin="564,41,0,0" TextWrapping="Wrap" Text="Recherche" VerticalAlignment="Top" Width="208"/>
        <Image HorizontalAlignment="Left" Height="72" Margin="26,22,0,0" VerticalAlignment="Top" Width="59" Source="/user (1).png"/>
        <Label x:Name="lb_nom_client" Content="Nom" HorizontalAlignment="Left" Height="30" Margin="101,22,0,0" VerticalAlignment="Top" Width="196" RenderTransformOrigin="0.508,-0.172" AutomationProperties.IsOffscreenBehavior="Default"/>
        <Label x:Name="lb_prenom_client" Content="Prénom" HorizontalAlignment="Left" Height="30" Margin="101,52,0,0" VerticalAlignment="Top" Width="196" RenderTransformOrigin="0.508,-0.172" AutomationProperties.IsOffscreenBehavior="Default"/>
        <Label x:Name="lb_nbr_arbre" Content="x Arbres" HorizontalAlignment="Left" Height="35" Margin="343,32,0,0" VerticalAlignment="Top" Width="168"/>
        <Button x:Name="bt_rapport" Content="Télecharger le rapport" HorizontalAlignment="Left" Height="23" Margin="648,383,0,0" VerticalAlignment="Top" Width="124"/>
        <Image HorizontalAlignment="Left" Height="310" Margin="-1,124,0,0" VerticalAlignment="Top" Width="410" Source="/carte.jpg"/>
        <StackPanel HorizontalAlignment="Left" Height="254" Margin="404,124,0,0" VerticalAlignment="Top" Width="368" Background="#FFF1EAEA"/>

    </Grid>
</Window>
